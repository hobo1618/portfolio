{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "image-gallery#build": {
      "dependsOn": ["^build", "$DB_PASS", "$DB_USER"],
      "env": ["DB_USER", "DB_PASS"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [".env"],
  "globalEnv": ["DB_USER", "DB_PASS"]
}